doctype html
html(lang='en')
  head
    title SKOLZ - Intro
    meta(charset='UTF-8')
    meta(name='viewport' content='width=device-width, initial-scale=1')
    //- script(src="https://cdn.jsdelivr.net/npm/vue@2.6.12")
    script(type="text/javascript" src="./vendor.js")
    link(rel='preload' href='./index.js' as='script')
    //- link(rel='preload' href='./vendor.js' as='script')
    link(rel='preload' href='styles.index.css' as='style')
