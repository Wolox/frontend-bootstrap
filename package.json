    "concurrent-transform": "^1.0.0",
    "connect-history-api-fallback": "^1.1.0",
    "del": "^2.2.0",
    "eslint": "^3.6.1",
    "eslint-config-angular": "^0.5.0",
    "eslint-plugin-angular": "^1.0.1",
