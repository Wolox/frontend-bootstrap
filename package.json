{
  "name": "frontend-bootstrap",
  "version": "0.0.1",
  "engines": {
    "node": "8.5.0",
    "npm": "5.3.0"
  },
  "description": "Kickoff for web applications",
  "scripts": {
    "start": "webpack-dev-server --config webpack.config.js",
    "build": "webpack --config webpack.config.prod.js",
    "pug-diff": "git diff --name-only --cached --relative | grep \\\\.pug$ | xargs ./node_modules/.bin/pug-lint",
    "lint-diff": "git diff --name-only --cached --relative | grep \\.js$ | xargs ./node_modules/eslint/bin/eslint.js",
    "precommit": "npm run pug-diff && npm run lint-diff",
    "lint": "sass-lint 'src/**/*.scss' -v -q"
  },
  "cacheDirectories": [
    "node_modules"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Wolox/frontend-bootstrap.git"
  },
  "author": "Sebastian Balay <sebastian.balay@wolox.com.ar>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Wolox/frontend-bootstrap/issues"
  },
  "homepage": "https://github.com/Wolox/frontend-bootstrap",
  "dependencies": {
    "eslint-plugin-vue": "^4.5.0",
    "vue": "^2.5.16"
  },
  "devDependencies": {
    "autoprefixer": "^8.2.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-register": "^6.8.0",
    "backstopjs": "^2.3.7",
    "browser-sync": "^2.12.8",
    "clean-webpack-plugin": "^0.1.19",
    "concurrent-transform": "^1.0.0",
    "css-loader": "^0.28.11",
    "del": "^2.2.0",
    "eslint": "^4.19.1",
    "extract-loader": "^2.0.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "glob": "^7.1.2",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "image-webpack-loader": "^4.2.0",
    "merge-stream": "^1.0.1",
    "minimatch": "^3.0.4",
    "node-notifier": "^5.0.2",
    "node-sass": "^4.8.3",
    "postcss-flexbugs-fixes": "^3.3.0",
    "postcss-loader": "^2.1.3",
    "prompt": "^1.0.0",
    "pug": "^2.0.3",
    "pug-html-loader": "^1.1.5",
    "replace": "^0.3.0",
    "require-dir": "^0.3.2",
    "run-sequence": "^1.1.5",
    "sass-loader": "^6.0.7",
    "stream-series": "^0.1.1",
    "style-loader": "^0.20.3",
    "thread-loader": "^1.1.5",
    "webpack": "^4.4.1",
    "webpack-cli": "^2.0.13",
    "webpack-dashboard": "^1.1.1",
    "webpack-dev-server": "^3.1.1",
    "webpack-serve": "^0.3.1"
  }
}
