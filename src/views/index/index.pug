          .social
            a.social-linkedin.social-link.m-right-4(href="https://www.linkedin.com/in/skolz/" rel="noreferrer noopener" target="_blank")
              linkedin-icon.social-icon
            a.social-github.social-link.m-right-4(href="https://github.com/SKOLZ" rel="noreferrer noopener" target="_blank")
              github-icon.social-icon
            a.social-twitter.social-link(href="https://twitter.com/WSKOLZ" rel="noreferrer noopener" target="_blank")
              twitter-icon.social-icon
        profile-image.profile-image
    script(src='./index.js')
