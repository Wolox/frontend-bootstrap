{
  "name": "frontend-bootstrap",
  "version": "1.0.0",
  "engines": {
    "node": "8.5.0",
    "npm": "5.3.0"
  },
  "description": "Kickoff for Vue.js web applications",
  "scripts": {
    "start": "webpack-dev-server --config webpack.config.js",
    "build": "webpack --config webpack.config.prod.js",
    "pug-diff": "git diff --name-only --cached --relative | grep \\\\.pug$ | xargs ./node_modules/.bin/pug-lint-vue -c ./.pug-lintrc.json",
    "lint-diff": "git diff --name-only --cached --relative | grep \\.js$ | xargs ./node_modules/eslint/bin/eslint.js",
    "precommit": "npm run pug-diff && npm run lint-diff",
    "lint": "sass-lint 'src/**/*.scss' -v -q",
    "create-view": "vg view",
    "create-component": "vg component"
  },
  "cacheDirectories": [
    "node_modules"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Wolox/frontend-bootstrap.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Wolox/frontend-bootstrap/issues"
  },
  "homepage": "https://github.com/Wolox/frontend-bootstrap",
  "dependencies": {
    "vue": "^2.5.17",
    "wolox-equalizer": "0.0.2"
  },
  "devDependencies": {
    "autoprefixer": "^8.2.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^7.1.4",
    "babel-plugin-dynamic-import-webpack": "^1.0.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-env": "^1.6.1",
    "backstopjs": "^2.3.7",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^0.28.11",
    "dotenv-webpack": "^1.5.7",
    "eslint": "^4.19.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-vue": "^4.5.0",
    "extract-loader": "^2.0.1",
    "file-loader": "^1.1.11",
    "glob": "^7.1.2",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "image-webpack-loader": "^4.2.0",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.8.3",
    "optimize-css-assets-webpack-plugin": "^4.0.1",
    "postcss-flexbugs-fixes": "^3.3.0",
    "postcss-loader": "^2.1.3",
    "prompt": "^1.0.0",
    "pug": "^2.0.3",
    "pug-html-loader": "^1.1.5",
    "pug-lint": "^2.5.0",
    "pug-lint-loader": "0.0.4",
    "pug-lint-vue": "^0.1.3",
    "pug-plain-loader": "^1.0.0",
    "replace": "^0.3.0",
    "require-dir": "^0.3.2",
    "resolve-url-loader": "^3.0.0",
    "sass-lint": "^1.12.1",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.3",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "url-loader": "^1.0.1",
    "vue-generator": "^1.1.0",
    "vue-loader": "^15.4.2",
    "vue-pug-lint-loader": "0.0.1",
    "vue-template-compiler": "^2.5.17",
    "webpack": "^4.4.1",
    "webpack-bundle-analyzer": "^2.11.1",
    "webpack-cli": "^2.0.13",
    "webpack-dashboard": "^1.1.1",
    "webpack-dev-server": "^3.1.1",
    "webpack-monitor": "^1.0.14",
    "webpack-serve": "^0.3.1",
    "workbox-webpack-plugin": "^3.6.2"
  },
  "sassLintConfig": ".sass-lint.yml",
  "pugLingConfig": ".pug-lintrc"
}
